    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "mobx-react-router": "^4.0.1",
    "pre-commit": "^1.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.1.6",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
